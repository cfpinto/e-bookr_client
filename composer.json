{
    "name": "bookr_com/e-bookr_client",
    "description": "Base structure for clients",
    "type": "library",
    "authors": [
        {
            "name": "Claudio Pinto",
            "email": "me@cfpinto.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "laravel/framework": "~5.5",
        "tcg/voyager": "^1.1",
        "barryvdh/laravel-translation-manager": ">=0.5"
    },
    "autoload": {
        "psr-4": {
            "Ebookr\\Client\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ebookr\\Client\\Providers\\ClientProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
